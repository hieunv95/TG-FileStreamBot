{
  "env": {
    "CGO_ENABLED": "1",
    "CGO_CFLAGS": "-I/vercel/path0/tdlib/include",
    "CGO_LDFLAGS": "-L/vercel/path0/tdlib/lib -ltdjson -lcrypto -lssl",
    "LD_LIBRARY_PATH": "/vercel/path0/tdlib/lib"
  },
  "build": {
    "env": {
      "CGO_ENABLED": "1",
      "CGO_CFLAGS": "-I/vercel/path0/tdlib/include",
      "CGO_LDFLAGS": "-L/vercel/path0/tdlib/lib -ltdjson -lcrypto -lssl",
      "GO_BUILD_FLAGS": "-ldflags '-s -w'"
    }
  },
  "routes": [
    {
      "src": "/webhook",
      "dest": "api/webhook.go"
    },
    {
      "src": "/file/(.*)",
      "dest": "api/file.go"
    }
  ]
}