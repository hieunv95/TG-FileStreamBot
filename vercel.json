{
  "env": {
    "CGO_ENABLED": "1",
    "CGO_CFLAGS": "-I/home/vercel/tdlib/include",
    "CGO_LDFLAGS": "-L/home/vercel/tdlib/lib -ltdjson"
  },
  "build": {
    "env": {
      "CGO_ENABLED": "1",
      "CGO_CFLAGS": "-I/home/vercel/tdlib/include",
      "CGO_LDFLAGS": "-L/home/vercel/tdlib/lib -ltdjson",
      "GO_BUILD_FLAGS": "-ldflags '-s -w'"
    }
  },
  "builds": [
    {
    "src": "api/build.sh",
    "use": "@vercel/static-build",
    "config": { "distDir": "vercel/output" }
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "api/webhook.go"
    },
    {
      "src": "/file/(.*)",
      "dest": "api/file.go"
    }
  ]
}