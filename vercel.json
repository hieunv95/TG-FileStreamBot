{
  "env": {
    "CGO_ENABLED": "1",
    "CGO_CFLAGS": "-I/vercel/tdlib/include",
    "CGO_LDFLAGS": "-L/vercel/tdlib/lib -ltdjson"
  },
  "build": {
    "env": {
      "CGO_ENABLED": "1",
      "CGO_CFLAGS": "-I/vercel/tdlib/include",
      "CGO_LDFLAGS": "-L/vercel/tdlib/lib -ltdjson",
      "GO_BUILD_FLAGS": "-ldflags '-s -w'"
    }
  },
  "builds": [
    {
    "src": "build.sh",
    "use": "@vercel/static-build",
    "config": { "distDir": "vercel/output" }
    }
  ],
  "routes": [
    { "src": "/api/webhook", "dest": "/vercel/output/webhook" },
    { "src": "/api/file", "dest": "/vercel/output/file" }
  ]
}